{
  "name": "lazyhustler-agi",
  "version": "1.0.0",
  "description": "Ultimate AGI Platform - Beats Manus.im with 150+ connectors and self-improving capabilities",
  "main": "backend/api.py",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:frontend & python3 backend/api.py",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "npm run build && npm run start:frontend & python3 backend/api.py",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "python3 backend/api.py",
    "test": "pytest",
    "test:frontend": "cd frontend && npm test",
    "lint": "eslint . --ext .ts,.tsx && black .",
    "format": "prettier --write . && black .",
    "docker:build": "docker build -t lazyhustler-agi .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "install:all": "npm install && cd frontend && npm install && cd .."
  },
  "keywords": [
    "agi",
    "autonomous",
    "ai",
    "agents",
    "manus",
    "automation",
    "learning"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  }
}
